double i = 1
while(i <= 100)
{
  if(i - (i / 15 * 15) == 0)
  {
    $"FizzBuzz"
    ;
    i += 1
    continue
  }
  if(i - (i / 3 * 3) == 0)
  {
    $"Fizz"
    ;
    i += 1
    continue
  }
  if(i - (i / 5 * 5) == 0)
  {
    $"Buzz"
    ;
    i += 1
    continue
  }
  $i
  ;
  i += 1
}
